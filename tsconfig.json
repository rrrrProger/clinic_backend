{
  "compilerOptions": {
    "baseUrl": "./cloud_clinic_frontend",
    "target": "esnext",
    "useDefineForClassFields": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "jsx": "react-jsxdev",
    "sourceMap": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom", "dom.iterable"],
    "types": [
      "node"
    ],
    "allowJs": true,
    "skipLibCheck": true,
  },
  "include": [
    "./cloud_clinic_frontend/src/**/*.ts",
    "./cloud_clinic_frontend/src/**/*.d.ts",
    "./cloud_clinic_frontend/src/**/*.tsx",
    "./cloud_clinic_frontend/src/**/*.vue",
    "./cloud_clinic_frontend/src/*",
    "cloud_clinic_frontend/vite.config.ts",
    "cloud_clinic_frontend"
  ]
}
